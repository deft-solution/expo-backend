{
  "name": "typescript-rest",
  "version": "1.0.0",
  "description": "Expo Backend",
  "main": "index.js",
  "scripts": {
    "htmlcopy": "copyfiles -u 1 \"src/**/*.html\" dist/src",
    "build": "rm -rf dist && tsc && npm run htmlcopy ",
    "watch-debug": "nodemon --inspect ./dist/src/server.js",
    "debug": "npm run build && npm run watch-debug",
    "start": "npm run build && ts-node-dev --inspect=9229 --transpile-only --ignore-watch node_modules ./src/server.ts"
  },
  "keywords": [
    "Rest",
    "API",
    "Typescript"
  ],
  "author": "Puty YAN",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "handlebars": "^4.7.8",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^8.3.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.0",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.15",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  }
}
